{
  "routes": [
    {
      "src": "/latest.apk",
      "dest": "/latest.apk",
      "headers": {
        "Content-Type": "application/vnd.android.package-archive",
        "Content-Disposition": "attachment; filename=latest.apk",
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/version.json",
      "dest": "/version.json"
    }
  ]
}
